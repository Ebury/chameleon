{
  "name": "@ebury/chameleon-components",
  "version": "1.0.13",
  "main": "src/main.js",
  "sideEffects": false,
  "author": "Ebury Team (http://labs.ebury.rocks/)",
  "license": "MIT",
  "homepage": "https://chameleon.ebury.now.sh/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebury/chameleon.git"
  },
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "icons": "webpack --config ./src/icons/webpack.config.js -p",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "vue-cli-service lint -- ./",
    "lint:style": "vue-cli-service lint:style",
    "test": "vue-cli-service test:unit --max-workers=2",
    "test:watch": "vue-cli-service test:unit --watch",
    "test:visual": "cypress open",
    "test:visual:ci": "npm run start & wait-on http://localhost:42475/ && npm run test:visual:all",
    "test:visual:all": "cypress run --browser chrome --headless --config video=true,videosFolder=tests/integration/videos/chrome/,trashAssetsBeforeRuns=true && cypress run --browser firefox --headless --config video=true,videosFolder=tests/integration/videos/firefox/,trashAssetsBeforeRuns=false",
    "prebuild-storybook": "npm run icons",
    "build-storybook": "build-storybook -s ./public,./src/assets -o .out --no-dll",
    "prestart": "npm run icons",
    "start": "start-storybook -s ./public,./src/assets -p 42475",
    "now-build": "npm run build-storybook",
    "prerelease": "npm run lint && npm run test",
    "release": "npm version patch && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "clipboard-copy": "3.1.0",
    "tailwindcss-filters": "3.0.0",
    "focus-trap": "5.1.0",
    "tailwindcss": "1.8.7",
    "v-tooltip": "2.0.3",
    "vue": "2.6.12",
    "vue-hoc": "0.4.7"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "1.1.2",
    "@fullhuman/postcss-purgecss": "2.3.0",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-essentials": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addon-knobs": "6.0.21",
    "@storybook/addon-a11y": "6.0.21",
    "@storybook/addon-cssresources": "6.0.21",
    "@storybook/addon-viewport": "6.0.21",
    "@storybook/source-loader": "6.0.21",
    "@storybook/theming": "6.0.21",
    "@storybook/addons": "6.0.21",
    "@storybook/vue": "6.0.21",
    "@testing-library/jest-dom": "5.11.4",
    "@vue/cli-plugin-babel": "4.5.6",
    "@vue/cli-plugin-eslint": "4.5.6",
    "@vue/cli-plugin-unit-jest": "4.5.6",
    "@vue/cli-service": "4.5.6",
    "@vue/eslint-config-airbnb": "5.1.0",
    "@vue/test-utils": "1.1.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-preset-vue": "2.0.2",
    "color": "3.1.2",
    "css-tree": "1.0.0-alpha.39",
    "css-vars-ponyfill": "2.3.2",
    "cypress": "5.1.0",
    "cypress-image-snapshot": "3.1.1",
    "eslint": "7.8.1",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-jest": "24.0.0",
    "eslint-plugin-vue": "6.2.2",
    "intl": "1.2.5",
    "lint-staged": "10.3.0",
    "postcss-import": "12.0.1",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.3",
    "postcss-preset-env": "6.7.0",
    "snapshot-diff": "0.8.1",
    "storybook-vue-router": "1.0.7",
    "stylelint": "13.7.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-standard-scss": "1.1.0",
    "stylelint-scss": "3.18.0",
    "stylelint-webpack-plugin": "2.1.0",
    "svg-sprite-loader": "5.0.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "url-loader": "4.1.0",
    "vue-loader": "15.9.3",
    "vue-router": "3.4.3",
    "vue-template-compiler": "2.6.12",
    "wait-on": "5.2.0",
    "webpack-cli": "3.3.12",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,htm,html,css}": [
      "npm run lint:style",
      "git add"
    ],
    "*.{vue,js}": [
      "npm run lint",
      "git add"
    ]
  }
}
