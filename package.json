{
  "name": "@ebury/chameleon-components",
  "version": "1.7.4",
  "main": "src/main.js",
  "sideEffects": false,
  "author": "Ebury Team (http://labs.ebury.rocks/)",
  "license": "MIT",
  "homepage": "https://chameleon.ebury.now.sh/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebury/chameleon.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "icons": "webpack --config ./src/icons/webpack.config.js --mode=production",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "vue-cli-service lint -- ./",
    "lint:style": "vue-cli-service lint:style",
    "test": "vue-cli-service test:unit --max-workers=2",
    "test:watch": "vue-cli-service test:unit --watch",
    "test:visual": "cypress open",
    "test:visual:ci": "npm run start & wait-on http://localhost:42475/ && npm run test:visual:all",
    "test:visual:all": "cypress run --browser chrome --headless --config video=true,videosFolder=tests/integration/videos/chrome/,trashAssetsBeforeRuns=true && cypress run --browser firefox --headless --config video=true,videosFolder=tests/integration/videos/firefox/,trashAssetsBeforeRuns=false",
    "prebuild-storybook": "npm run icons",
    "build-storybook": "build-storybook -s ./public,./src/assets -o .out",
    "prestart": "npm run icons",
    "start": "start-storybook -s ./public,./src/assets -p 42475",
    "now-build": "npm run build-storybook",
    "prerelease": "npm run lint && npm run test",
    "release": "npm version patch && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "clipboard-copy": "3.2.0",
    "focus-trap": "6.6.1",
    "mitt": "3.0.0",
    "tailwindcss": "2.2.14",
    "v-tooltip": "2.1.3",
    "vue": "2.6.14",
    "vue-hoc": "0.4.7"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "1.1.2",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@sinonjs/fake-timers": "7.1.2",
    "@storybook/addon-a11y": "6.3.8",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-cssresources": "6.2.9",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-knobs": "6.3.1",
    "@storybook/addon-links": "6.3.8",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-viewport": "6.3.8",
    "@storybook/addons": "6.3.8",
    "@storybook/source-loader": "6.3.8",
    "@storybook/theming": "6.3.8",
    "@storybook/vue": "6.3.8",
    "@testing-library/jest-dom": "5.14.1",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/test-utils": "1.2.2",
    "autoprefixer": "10.3.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.1.0",
    "babel-loader": "8.2.2",
    "babel-preset-vue": "2.0.2",
    "color": "4.0.1",
    "css-tree": "1.1.3",
    "css-vars-ponyfill": "2.4.7",
    "cypress": "5.6.0",
    "cypress-image-snapshot": "4.0.1",
    "eslint": "7.32.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-vue": "7.18.0",
    "intl": "1.2.5",
    "lint-staged": "11.1.2",
    "postcss": "8.3.7",
    "postcss-import": "14.0.2",
    "postcss-mixins": "8.1.0",
    "postcss-nested": "5.0.6",
    "purgecss": "4.0.3",
    "purgecss-from-html": "4.0.3",
    "snapshot-diff": "0.8.1",
    "storybook-vue-router": "1.0.7",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-standard-scss": "1.1.0",
    "stylelint-scss": "3.21.0",
    "stylelint-webpack-plugin": "3.0.1",
    "svg-sprite-loader": "5.2.1",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.2",
    "url-loader": "4.1.1",
    "vue-loader": "15.9.8",
    "vue-router": "3.5.2",
    "vue-template-compiler": "2.6.14",
    "wait-on": "6.0.0",
    "webpack-cli": "4.8.0",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,htm,html,css}": [
      "vue-cli-service lint:style",
      "git add"
    ],
    "*.{vue,js}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
