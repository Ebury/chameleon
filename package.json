{
  "name": "@ebury/chameleon-components",
  "version": "2.1.27",
  "main": "src/main.js",
  "sideEffects": false,
  "author": "Ebury Team (http://labs.ebury.rocks/)",
  "license": "MIT",
  "homepage": "https://chameleon-ebury.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebury/chameleon.git"
  },
  "engines": {
    "node": "16"
  },
  "scripts": {
    "icons": "webpack --config ./src/icons/webpack.config.js --mode=production",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:no-fix": "npm run lint:js:no-fix && npm run lint:style:no-fix",
    "lint:js": "eslint --fix ./",
    "lint:js:no-fix": "eslint --no-fix ./",
    "lint:style": "stylelint ./src/**/*.{css,vue} --fix",
    "lint:style:no-fix": "stylelint ./src/**/*.{css,vue}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:visual": "cypress open",
    "test:visual:ci": "npm run build-storybook-run & wait-on http://localhost:42475/ && npm run test:visual:all",
    "test:visual:all": "cypress run --browser chrome --headless --config video=false,videosFolder=tests/integration/videos/chrome/,trashAssetsBeforeRuns=true && cypress run --browser firefox --headless --config video=false,videosFolder=tests/integration/videos/firefox/,trashAssetsBeforeRuns=false",
    "prebuild-storybook": "npm run icons",
    "build-storybook": "vue-tsc --noEmit && build-storybook -o .out",
    "build-storybook-run": "npm run build-storybook && live-server --port=42475 .out",
    "prestart": "npm run icons",
    "start": "vue-tsc --noEmit --watch & start-storybook -p 42475",
    "serve": "npm run start",
    "prerelease": "npm run lint && npm run test",
    "release": "npm version patch && git tag -d v$(npm view . version) && npm publish && git push",
    "git-tag": "git tag v$(npm view . version) && git push origin v$(npm view . version)"
  },
  "dependencies": {
    "@vueuse/core": "9.2.0",
    "@vueuse/integrations": "9.2.0",
    "clipboard-copy": "3.2.0",
    "flatpickr": "4.6.13",
    "floating-vue": "2.0.0-beta.16",
    "focus-trap": "6.9.4",
    "mitt": "3.0.0",
    "svg-country-flags": "1.2.10",
    "tailwindcss": "2.2.19",
    "vue": "3.2.39"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@sinonjs/fake-timers": "9.1.2",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-cssresources": "6.2.9",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-viewport": "6.5.12",
    "@storybook/addons": "6.5.12",
    "@storybook/builder-vite": "0.2.3",
    "@storybook/source-loader": "6.5.12",
    "@storybook/theming": "6.5.12",
    "@storybook/vue3": "6.5.12",
    "@testing-library/jest-dom": "5.16.5",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@vitejs/plugin-vue": "3.1.2",
    "@vue/babel-preset-app": "5.0.8",
    "@vue/eslint-config-airbnb": "6.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.0.2",
    "@vue/tsconfig": "0.1.3",
    "@vue/vue3-jest": "29.2.0",
    "autoprefixer": "10.4.12",
    "color": "4.2.3",
    "css-tree": "2.2.1",
    "cypress": "10.11.0",
    "cypress-image-snapshot": "4.0.1",
    "eslint": "8.24.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-vue": "9.5.1",
    "eslint-plugin-vuejs-accessibility": "1.2.0",
    "intl": "1.2.5",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-serializer-vue": "2.0.2",
    "lint-staged": "13.0.3",
    "live-server": "1.2.2",
    "postcss": "8.4.16",
    "postcss-html": "1.5.0",
    "postcss-import": "14.1.0",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "5.0.6",
    "purgecss": "4.0.3",
    "purgecss-from-html": "4.0.3",
    "snapshot-diff": "0.9.0",
    "storybook-vue3-router": "2.3.1",
    "stylelint": "14.12.1",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "28.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "svg-sprite-loader": "6.0.11",
    "svgo": "2.8.0",
    "svgo-loader": "3.0.1",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4",
    "vite": "3.0.9",
    "vue-eslint-parser": "9.1.0",
    "vue-router": "4.1.5",
    "vue-tsc": "0.40.1",
    "wait-on": "6.0.1",
    "webpack-cli": "4.10.0",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,css}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{vue,js,jsx}": [
      "eslint --fix ./",
      "git add"
    ]
  }
}
