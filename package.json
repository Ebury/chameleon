{
  "name": "@ebury/chameleon-components",
  "version": "0.1.24",
  "main": "src/main.js",
  "sideEffects": false,
  "author": "Ebury Team (http://labs.ebury.rocks/)",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "icons": "webpack --config ./src/icons/webpack.config.js -p",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "vue-cli-service lint -- ./",
    "lint:style": "vue-cli-service lint:style",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "prebuild-storybook": "npm run icons",
    "build-storybook": "build-storybook -s ./public,./src/assets -o .out --no-dll",
    "prestart": "npm run icons",
    "start": "start-storybook -s ./public,./src/assets -p 42475",
    "now-build": "npm run build-storybook",
    "prerelease": "npm run lint && npm run test",
    "release": "npm version patch && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "core-js": "2.6.5",
    "focus-trap": "5.1.0",
    "v-tooltip": "2.0.2",
    "vue": "2.6.10"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "1.1.2",
    "@storybook/addon-a11y": "5.2.6",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-backgrounds": "5.2.6",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-storysource": "5.2.6",
    "@storybook/addon-viewport": "5.2.6",
    "@storybook/theming": "5.2.6",
    "@storybook/vue": "5.2.6",
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/cli-plugin-eslint": "3.11.0",
    "@vue/cli-plugin-unit-jest": "3.11.0",
    "@vue/cli-service": "3.11.0",
    "@vue/eslint-config-airbnb": "4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.6",
    "babel-preset-vue": "2.0.2",
    "clipboard-copy": "3.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-jest": "22.20.0",
    "eslint-plugin-vue": "5.2.3",
    "lint-staged": "9.4.2",
    "sass": "1.23.3",
    "sass-loader": "7.1.0",
    "snapshot-diff": "0.5.2",
    "storybook-vue-router": "1.0.7",
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-config-standard-scss": "1.1.0",
    "stylelint-scss": "3.11.1",
    "svg-sprite-loader": "4.1.6",
    "svgo": "1.3.0",
    "svgo-loader": "2.2.1",
    "vue-loader": "15.7.1",
    "vue-router": "3.1.3",
    "vue-template-compiler": "2.6.10",
    "webpack-cli": "3.3.9",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,htm,html,css,scss}": [
      "vue-cli-service lint:style",
      "git add"
    ],
    "*.{vue,js}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
