{
  "name": "@ebury/chameleon-components",
  "version": "0.1.80",
  "main": "src/main.js",
  "sideEffects": false,
  "author": "Ebury Team (http://labs.ebury.rocks/)",
  "license": "MIT",
  "homepage": "https://chameleon.ebury.now.sh/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebury/chameleon.git"
  },
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "icons": "webpack --config ./src/icons/webpack.config.js -p",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "vue-cli-service lint -- ./",
    "lint:style": "vue-cli-service lint:style",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "prebuild-storybook": "npm run icons",
    "build-storybook": "build-storybook -s ./public,./src/assets -o .out --no-dll",
    "prestart": "npm run icons",
    "start": "start-storybook -s ./public,./src/assets -p 42475",
    "now-build": "npm run build-storybook",
    "prerelease": "npm run lint && npm run test",
    "release": "npm version patch && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "focus-trap": "5.1.0",
    "v-tooltip": "2.0.3",
    "vue": "2.6.11",
    "vue-hoc": "0.4.7"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "1.1.2",
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-backgrounds": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/theming": "5.3.18",
    "@storybook/vue": "5.3.18",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-plugin-unit-jest": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-airbnb": "5.0.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.4.0",
    "babel-loader": "8.1.0",
    "babel-preset-vue": "2.0.2",
    "clipboard-copy": "3.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-vue": "6.2.2",
    "intl": "1.2.5",
    "lint-staged": "10.1.6",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "snapshot-diff": "0.7.0",
    "storybook-vue-router": "1.0.7",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-standard-scss": "1.1.0",
    "stylelint-scss": "3.17.1",
    "svg-sprite-loader": "4.2.5",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "vue-loader": "15.9.1",
    "vue-router": "3.1.6",
    "vue-template-compiler": "2.6.11",
    "webpack-cli": "3.3.11",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,htm,html,css,scss}": [
      "vue-cli-service lint:style",
      "git add"
    ],
    "*.{vue,js}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
